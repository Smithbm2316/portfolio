{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@data/*": ["data/*"],
      "@layouts/*": ["layouts/*"],
      "@migrations/*": ["migrations/*"],
      "@models/*": ["models/*"],
      "@pages/*": ["pages/*"]
    }
  }
}
