{
  "$schema": "https://json.schemastore.org/jsconfig.json",
  "compilerOptions": {
    "checkJs": true,
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "noImplicitAny": false,
    "target": "es2022",
    "moduleResolution": "nodenext",
    "baseUrl": ".",
    "paths": {
      "#config/*": ["./config/*"],
      "#server/*": ["./server/*"],
      "#src/*": ["./src/*"]
    }
  },
  "include": ["eleventy.config.js", "config/**/*.js", "src/**/*.js", "global.d.ts"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
